{
    "node": true,
    "bitwise": true,
    "camelcase": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 2,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "smarttabs": true,
    "white": true,

    "overrides": {
      "test/**/global*.js": {
        "strict": false,
        "unused": false
      }
    },

    "globals": {
        "addFunc": true,
        "missedFunc": true
    }
}
